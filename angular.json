{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@dorotea/mia-core": {
      "projectType": "library",
      "root": "projects/dorotea/mia-core",
      "sourceRoot": "projects/dorotea/mia-core/src",
      "prefix": "mia",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/dorotea/mia-core/tsconfig.lib.json",
            "project": "projects/dorotea/mia-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dorotea/mia-core/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dorotea/mia-core/src/test.ts",
            "tsConfig": "projects/dorotea/mia-core/tsconfig.spec.json",
            "karmaConfig": "projects/dorotea/mia-core/karma.conf.js"
          }
        }
      }
    }},
  "cli": {
    "analytics": "d938046f-3aa7-499d-9e1b-e56410309cf3"
  },
  "defaultProject": "@dorotea/mia-core"
}
